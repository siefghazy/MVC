
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using compant.Data.Models
@model ApplicationUser
<form asp-action="Edit">
    <div class="form-group">
        <label hidden asp-for="Id" class="col-form-label"></label>
        <input hidden asp-for="Id" class="form-control" />
        <span asp-validation-for="Id" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="UserName" class="col-form-label"></label>
        <input asp-for="UserName" class="form-control" />
        <span asp-validation-for="UserName" class="text-danger"></span>
    </div>
    <input type="submit" value="Save" class="btn btn-primary" />
    <a asp-controller="User" asp-action="Index" class="btn btn-light">Back To List</a>
</form>
